{
  "name": "berghain-bot",
  "version": "0.1.0",
  "private": true,
  "description": "Phase 1 minimal playable bot for the Berghain Challenge.",
  "license": "MIT",
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "build": "tsc -p .",
    "start": "node dist/index.js",
    "dev": "ts-node src/index.ts",
    "play": "ts-node src/index.ts",
    "web": "ts-node src/web/server.ts",
    "scenario:1": "cross-env SCENARIO=1 ts-node src/index.ts",
    "scenario:2": "cross-env SCENARIO=2 ts-node src/index.ts",
    "scenario:3": "cross-env SCENARIO=3 ts-node src/index.ts"
  },
  "dependencies": {
    "axios": "^1.7.7",
    "dotenv": "^16.4.5",
    "express": "^4.21.2",
    "socket.io": "^4.8.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.23",
    "@types/node": "^20.14.12",
    "cross-env": "^7.0.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.4"
  }
}
